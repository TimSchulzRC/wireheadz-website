{
  "id": "homepage",
  "label": "Homepage",
  "repeatable": false,
  "status": true,
  "format": "page",
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "WireHeadZ Games Hub"
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Subtitle",
          "placeholder": "WHZ eSports ist mehr als nur Gaming."
        }
      },
      "links": {
        "type": "Link",
        "config": {
          "label": "Links",
          "placeholder": "",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": true,
          "variants": [
            "link",
            "secondary",
            "destructive",
            "outline",
            "ghost",
            "default"
          ]
